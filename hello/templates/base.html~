</script>

<script>

var map;

var coord = [[59.4012612,10.8030005],[59.4032612,10.8030005]]

function initialize()
{
var mapProp = {
  center:new google.maps.LatLng(59.4037705,10.8099957),
  zoom:14,
  mapTypeId:google.maps.MapTypeId.ROADMAP,
  disableDefaultUI: true
  };
map=new google.maps.Map(document.getElementById("googleMap")
  ,mapProp);
  
var marker=new google.maps.Marker({
  position:myCenter,
  });
  
marker.setMap(map);
}


function addmarkers()
{
  
for(i=0;i<coord.length; i++){
myCenter=new google.maps.LatLng(coord[i][0],coord[i][1]);

marker=new google.maps.Marker({
  position:myCenter,
  });
  
marker.setMap(map);

}  


}



google.maps.event.addDomListener(window, 'load', initialize);
google.maps.event.addDomListener(window, 'load', addmarkers);
</script>






